{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "paths": {
      "@config": [
        "./config/index.js"
      ],
      "@sounds/*": [
        "./sounds/*"
      ],
      "@app": [
        "src/AppContext.js"
      ],
      "@bips": [
        "src/modules/BipList/index.js"
      ],
      "@components": [
        "./src/components/index.js"
      ],
      "@contacts": [
        "./src/modules/Contacts/index.js"
      ],
      "@feed": [
        "./src/modules/Feed/index.js"
      ],
      "@forum": [
        "./src/modules/Forum/index.js"
      ],
      "@mail": [
        "./src/modules/Mail/index.js"
      ],
      "@modal": [
        "./src/modules/Modal/index.js"
      ],
      "@notification": [
        "./src/modules/Notification/index.js"
      ],
      "@play": [
        "./src/modules/Play/index.js"
      ],
      "@socket": [
        "src/modules/Socket/index.js"
      ],
      "@form": [
        "./src/modules/Form/index.js"
      ],
      "@forms": [
        "./src/modules/Form/forms/index.js"
      ],
      "@images/*": [
        "./src/modules/Images/*"
      ],
      "@layout": [
        "./src/Layout/index.js"
      ],
      "@modules": [
        "./src/modules/index.js"
      ],
      "@screens": [
        "./src/screens/index.js"
      ],
      "@styles": [
        "./src/styles/index.js"
      ],
      "@utils": [
        "./src/utils/index.js"
      ]
    },
    "preserveSymlinks": true,
    "target": "es6",
    "module": "ESNext",
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": [
      "es6",
      "dom"
    ],
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src"
  ],
  "extends": "@tsconfig/react-native/tsconfig.json"
  // "extends": "expo/tsconfig.base"
}
